{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "importOrder": [
    "^node:.*$",
    "^(react(/.*)?)$",
    "^(next(/.*)?)$",
    "<THIRD_PARTY_MODULES>",
    "^@/.*$",
    "^[./]"
  ],
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
